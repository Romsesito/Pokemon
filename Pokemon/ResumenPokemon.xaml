<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Pokemon.ResumenPokemon"
             Title="ResumenPokemon">
    <VerticalStackLayout HorizontalOptions="Center">

   

        <Image
            x:Name="ImagePokemon"
            HeightRequest="500"
            WidthRequest="500"/>

        <Frame Padding="10" BorderColor="#F84C40">
            <Label 
        x:Name="Habilidades"
                TextColor="White"
                FontAttributes="Bold"
                FontSize="20"
                />
           
        </Frame>

    </VerticalStackLayout>
</ContentPage>